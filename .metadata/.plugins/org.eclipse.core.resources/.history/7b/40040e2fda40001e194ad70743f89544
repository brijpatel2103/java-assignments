package com.maven.AppiumTest;

import java.net.URL;
import java.sql.Driver;

import org.openqa.selenium.remote.DesiredCapabilities;

import io.appium.java_client.AppiumDriver;

public class Gallery {
	static AppiumDriver driver=null;
	public static void main(String[] args) {
		openGallery();
	}
}



public static void openGallery()
{
	DesiredCapabilities cap=new DesiredCapabilities();
	cap.setCapability("deviceName", "Xiaomi POCO F1");
	cap.setCapability("udid", "915997cd");
	cap.setCapability("platformName", "Android");
	cap.setCapability("platformVersion", "10");
	cap.setCapability("appPackage", "com.miui.gallery");
	cap.setCapability("appActivity", "com.miui.gallery.activity.HomePageActivity");
	cap.setCapability("automationName", "UiAutomator2");
	URL url=new URL("http://127.0.0.1:4723/");
	driver=new AppiumDriver(url,cap);
	driver.quit();
}