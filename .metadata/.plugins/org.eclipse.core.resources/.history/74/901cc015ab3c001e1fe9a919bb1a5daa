package com.thread;
class callback
{
	public void call(String msg)
	{
		System.out.print("["+msg);
		try {
			Thread.sleep(1000);
		} catch (Exception e) {
			e.printStackTrace();
		}
		System.out.println("]");
	}
}
class caller implements Runnable
{
	String msg;
	Thread t;
	callback c;
	public caller(String msg,callback c) {
		this.msg=msg;
		this.c=c;
		t=new Thread(this);
		t.start();
		
	}

	public void run() {
		
	}
	
}
public class ThreadSycn {

}
